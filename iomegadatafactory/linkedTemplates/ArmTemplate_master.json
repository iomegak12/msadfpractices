{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "iomegadatafactory"
		},
		"AzureDLSLinkedService_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDLSLinkedService'"
		},
		"AzureSQLDBLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSQLDBLinkedService'"
		},
		"AzureSQLDWLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSQLDWLinkedService'"
		},
		"AzureBlobStorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageLinkedService'"
		},
		"AzureDLSLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://iomegadls.dfs.core.windows.net"
		},
		"AzureKVLinkedService_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://iomegakv.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDLSLinkedService_accountKey": {
						"value": "[parameters('AzureDLSLinkedService_accountKey')]"
					},
					"AzureBlobStorageLinkedService_connectionString": {
						"value": "[parameters('AzureBlobStorageLinkedService_connectionString')]"
					},
					"AzureSQLDBLinkedService_connectionString": {
						"value": "[parameters('AzureSQLDBLinkedService_connectionString')]"
					},
					"AzureSQLDWLinkedService_connectionString": {
						"value": "[parameters('AzureSQLDWLinkedService_connectionString')]"
					},
					"AzureDLSLinkedService_properties_typeProperties_url": {
						"value": "[parameters('AzureDLSLinkedService_properties_typeProperties_url')]"
					},
					"AzureKVLinkedService_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKVLinkedService_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}